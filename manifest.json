{
  "manifest_version": 3,

  "name": "yarnpm",
  "version": "0.0.1",
  "icons": {
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.npmjs.com/*"],
      "js": ["bundle.js"],
      "run_at": "document_idle"
    }
  ],
  "action": {
    "default_popup": "popup/index.html"
  }
}
